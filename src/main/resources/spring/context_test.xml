<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
       default-lazy-init="true"  >


    <bean class="ardasProject.DefineTimeLocal.Time" id="time"/>
    <bean class="ardasProject.DefineTimeLocal.DefineTimePartDay" id="defineTime"/>
    <bean class="ardasProject.service.GetLocal" id="getLocal"/>
    <bean class="ardasProject.start.StartProject" id="startProj"/>
    <bean class="ardasProject.service.ClientSettings" id="clientSettings"/>
    <bean class="ardasProject.myCash.SetCash" id="setCash"/>
    <bean class="ardasProject.myCash.CashMap" id="cashMap"/>
    <bean class="ardasProject.processing.SetCashProcessingLocale" id="setCashProcessingLocale"/>
    <bean class="ardasProject.showLocalTime.DisplayLocaleTimeRU" id="display_lt_ru"/>
    <bean class="ardasProject.showLocalTime.DisplayLocaleTimeUK" id="display_lt_uk"/>
    <bean class="ardasProject.showLocalTime.Display_LT_US" id="display_lt_en"/>
    <bean class="ardasProject.processing.RunnerStructure" id="runner"/>
    <bean class="ardasProject.service.DisplayHello" id="starterDisp"/>



    <context:annotation-config/>
    <context:component-scan base-package="ardas.projectstart"/>

</beans>